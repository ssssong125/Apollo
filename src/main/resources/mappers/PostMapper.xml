<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "com.b4.apollo.blog.post.model.dao.PostMapper">

    <!-- 게시글 등록 -->
    <insert id="insertBoard" parameterType="com.b4.apollo.qna.model.dto.QuestionDTO">
        INSERT INTO TBL_BLOG_BOARD
        VALUES
            (SEQ_BLOG_NO.NEXTVAL, 'USER01', #{blogTitle}, #{blogContent}, SYSDATE, DEFAULT, DEFAULT)
    </insert>




</mapper>